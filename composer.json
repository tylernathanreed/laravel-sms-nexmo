{
    "name": "reedware/laravel-sms-nexmo",
    "description": "Adds the ability to send text messages using Nexmo.",
    "keywords": ["laravel", "sms", "nexmo"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Reed",
            "email": "tylernathanreed@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "reedware/laravel-sms": "^0.3",
        "vonage/client-core": "^2.4"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.0",
        "mockery/mockery": "^1.4.2",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "orchestra/testbench-core": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.5|^9.5"
    },
    "autoload": {
        "psr-4": {
            "Reedware\\LaravelSMS\\Nexmo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reedware\\LaravelSMS\\Tests\\Nexmo\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reedware\\LaravelSMS\\Nexmo\\NexmoServiceProvider"
            ]
        }
    }
}
